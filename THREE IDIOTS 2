import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner inn = new Scanner(System.in);
    int x1 = inn.nextInt();
    int y1 = inn.nextInt();
    int x2 = inn.nextInt();
    int y2 = inn.nextInt();
    double midX = (x1 + x2) / 2.0;
    double midY = (y1 + y2) / 2.0;
    System.out.println("Binoy's house is located at (" + midX + "," + midY + ")");
   
    }
}
